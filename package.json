{
  "name": "data-miner",
  "version": "1.0.0",
  "description": "A simple Electron app to shuffle text",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^23.3.13",
    "electron-builder": "^22.11.7"
  },
  "author": "Seu Nome",
  "license": "MIT",
  "build": {
    "appId": "none.com",
    "productName": "Data miner app",
    "files": [
      "main.js",
      "index.html",
      "scrapeGoogleBusiness.js",
      "render.js",
      "preload.js",
      "logger.js",
      "node_modules/**/*",
      "browser/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "electron-log": "^5.1.7",
    "puppeteer-core": "^22.14.0",
    "uuid": "^10.0.0"
  }
}
